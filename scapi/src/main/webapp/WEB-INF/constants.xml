<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd"
	default-autowire="byName">
	
	<util:map id="resultCodeMap" map-class="java.util.HashMap" key-type="java.lang.Integer" value-type="java.lang.String">
   		<!-- normal success flow  -->
   		<entry key="200" value="" />
   		
   		<entry key="4001" value="FAIL : validation error" />
   		<entry key="4004" value="FAIL : no record is found" />
   		
   		<entry key="5000" value="FAIL : system error" />
   		<entry key="5001" value="FAIL : database error" />
   		<entry key="5002" value="FAIL : update is failed" />
   		<entry key="5003" value="FAIL : remove is failed" />

	</util:map>
	

	
</beans>
